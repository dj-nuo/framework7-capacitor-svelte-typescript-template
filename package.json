{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:ios": "ionic cap run ios -l --external",
    "ionic:serve": "vite dev --host",
    "ionic:build": "vite build",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "build-capacitor-ios": "cross-env NODE_ENV=production vite build && npx cap copy ios",
    "build-capacitor-android": "cross-env NODE_ENV=production vite build && npx cap copy android",
    "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/"
  },
  "dependencies": {
    "@capacitor/android": "^4.7.1",
    "@capacitor/app": "^4.1.1",
    "@capacitor/browser": "^4.1.1",
    "@capacitor/core": "^4.7.1",
    "@capacitor/ios": "^4.7.1",
    "@capacitor/keyboard": "^4.1.1",
    "@capacitor/splash-screen": "^4.2.0",
    "@capacitor/status-bar": "^4.1.1",
    "@tsconfig/svelte": "^3.0.0",
    "dom7": "^4.0.6",
    "framework7": "^7.1.5",
    "framework7-icons": "^5.0.5",
    "framework7-svelte": "^7.1.5",
    "material-icons": "^1.13.3",
    "skeleton-elements": "^4.0.1",
    "svelte": "^3.55.1",
    "swiper": "^8.4.7"
  },
  "devDependencies": {
    "@capacitor/cli": "^4.7.1",
    "cordova-res": "^0.15.4",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@tsconfig/svelte": "^3.0.0",
    "svelte": "^3.55.1",
    "svelte-check": "^2.10.3",
    "tslib": "^2.5.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  }
}